{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/glide-complete", "destination": "/api/glide-complete" }
  ],
  "functions": {
    "api/**": {
      "runtime": "@vercel/node@3.x"
    }
  }
}
