{
  "name": "magical-auth-quickstart-react",
  "version": "1.0.0",
  "description": "Magical Auth Quickstart for React with multi-backend support (Node.js, Go, Java)",
  "type": "module",
  "scripts": {
    "dev": "concurrently --raw \"npm run server:go\" \"npm run dev:react\"",
    "dev:go": "concurrently --raw \"npm run server:go\" \"npm run dev:react\"",
    "dev:node": "concurrently --raw \"npm run server:node\" \"npm run dev:react\"",
    "dev:java": "concurrently --raw \"npm run server:java\" \"npm run dev:react\"",
    "dev:react": "vite --config frontend/vite.config.js",
    "server:go": "cd server/go && GOWORK=off go mod download && GOWORK=off go run .",
    "server:node": "tsx watch server/node/index.ts",
    "server:java": "dotenv -e .env -- sh -c 'cd server/java && ./gradlew bootRun'",
    "server:java:build": "cd server/java && ./gradlew build",
    "server:java:clean": "cd server/java && ./gradlew clean",
    "server:go:build": "cd server/go && GOWORK=off go build -o ../../dist/server-go .",
    "postinstall": "cd server/go && GOWORK=off go mod download 2>/dev/null || echo 'Go not found - use npm run dev:node instead'",
    "build": "vite build --config frontend/vite.config.js",
    "build:server": "tsc",
    "build:all": "npm run build && npm run build:server && npm run server:go:build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@glideidentity/glide-be-sdk-node": "^2.1.1",
    "@glideidentity/glide-fe-sdk-web": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.0.6"
  }
}
